<div>

  <macro:HelloWorld name="string">
    <p>Hello, ${name}!</p>
  </macro:HelloWorld>

  <s:var nameVar="'world'"/>
  
  <content:middle>
    <p>two</p>
    !{HelloWorld(nameVar)}
    <p>three</p>
  </content:middle>

  <p>one</p>
  <use:middle/>
  <p>four</p>

  <var x="1/0">ignored</var>

</div>
