<html>
  <head>
    <title>foreach test</title>
  </head>
  <body>
    <h1>foreach test</h1>
    <p>${System.DateTime.Now}</p>
    <var items="new[]{'foo','bar','baaz'}" />
    <ul>
      <var css="new[]{'even','odd'}" i="0">
        <for each="var item in items" i="i+1">
          <li class="${css[i%2]}">${i}: ${item}</li>
        </for>
      </var>
    </ul>
  </body>
</html>
