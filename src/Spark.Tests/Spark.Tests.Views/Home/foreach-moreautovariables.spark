<table>
  <tr>
    <th>Item</th>
    <th>Index</th>
    <th>Count</th>
    <th>IsFirst</th>
    <th>IsLast</th>
  </tr>
  <tr each="var item in new [] {'one','two','three','four'}">
    <td>${item}</td>
    <td>${itemIndex}</td>
    <td>${itemCount}</td>
    <td>${itemIsFirst}</td>
    <td>${itemIsLast}</td>
    <td>${itemIsEven}</td>
    <td>${itemIsOdd}</td>
  </tr>
</table>
